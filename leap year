import java.util.*;

import java.lang.*;

import java.io.*;



class Ideone
{

	
public static void main (String[] args) throws java.lang.Exception
	
{
    
	
Scanner sc=new Scanner(System.in);
	
int N=sc.nextInt();
    
if(N%4==0 && N%100!=0 || N%400==0)
    
{
	
System.out.println("yes");
    
}

    
else{
	
System.out.println("no");
    
}

	
}
}
